{
  "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "moduleResolution": "Node",
      "strict": true,
      "noImplicitAny": true,
      "declaration": false,
      "skipLibCheck": true,
      "strictNullChecks": true,
      "resolveJsonModule": true,
      "outDir": "dist",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}